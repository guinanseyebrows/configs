XF86AudioMute
	amixer -c 1 -q sset PCM toggle 

XF86AudioLowerVolume
	amixer -c 1 -q sset PCM 5%-

XF86AudioRaiseVolume
	amixer -c 1 -q sset PCM 5%+

XF86AudioMicMute

XF86AudioPlay
    pyspctl playpause

XF86AudioNext
    pyspctl next

XF86AudioPrev
    pyspctl previous

XF86MonBrightnessDown
	xbacklight -dec 10

XF86MonBrightnessUp
	xbacklight -inc 10
